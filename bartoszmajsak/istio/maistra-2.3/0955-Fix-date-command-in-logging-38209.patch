From: John Howard <howardjohn@google.com>
Date: Thu, 31 Mar 2022 12:57:36 -0700
Subject: Fix date command in logging (#38209)

---
 prow/lib.sh | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/prow/lib.sh b/prow/lib.sh
index e317d65fa5..1734b0d0d3 100755
--- a/prow/lib.sh
+++ b/prow/lib.sh
@@ -18,10 +18,10 @@ function date_cmd() {
   case "$(uname)" in
     "Darwin")
       [ -z "$(which gdate)" ] && echo "gdate is required for OSX. Try installing coreutils from MacPorts or Brew."
-      echo "gdate"
+      gdate "$@"
       ;;
     *)
-      echo "date"
+      date "$@"
       ;;
   esac
 }
