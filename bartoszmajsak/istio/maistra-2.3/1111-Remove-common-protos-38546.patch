-    // The strategy associated with a rollout to delete a `ManagedService`.
-    // Readonly.
-    DeleteServiceStrategy delete_service_strategy = 200;
-  }
-
-  // The name of the service associated with this Rollout.
-  string service_name = 8;
-}
